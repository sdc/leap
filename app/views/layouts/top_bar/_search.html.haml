= form_tag search_people_url, :method => :get, :class => "navbar-form navbar-left" do
  .input-group
    = text_field_tag :q, params[:q], :class => "form-control", :placeholder => "Search..."
    %span.input-group-btn
      %button{:type => "submit", :class => "btn btn-default"}
        %i.fa.fa-search
        Go

